        const slidesPerView = window.innerWidth < 768 ? 1 : 3;
        updateCarouselButtons('prepaid', 0, slideCount, slidesPerView);
      }
    }

    // Function to slide plans
    function slidePlans(direction) {
      const carousel = document.getElementById('planCarousel');
      if (!carousel) return;
      
      const cards = carousel.children;
      const slideCount = cards.length;
      const slidesPerView = window.innerWidth < 768 ? 1 : 3;
      
      // Get current slide position
      const currentTransform = carousel.style.transform;
      let currentSlide = 0;
      if (currentTransform) {
        const match = currentTransform.match(/translateX\((-?\d+)px\)/);
        if (match && match[1]) {
          const cardWidth = cards[0]?.offsetWidth || 0;
          const gap = 16; // 1rem = 16px
          const slideWidth = cardWidth + gap;
          currentSlide = Math.round(Math.abs(parseInt(match[1])) / slideWidth);
        }
      }
      
      // Calculate the width of a single card including gap
      const cardWidth = cards[0]?.offsetWidth || 0;
      const gap = 16; // 1rem = 16px
      const slideWidth = cardWidth + gap;
      
      if (direction === 'next' && currentSlide < slideCount - slidesPerView) {
        currentSlide++;
      } else if (direction === 'prev' && currentSlide > 0) {
        currentSlide--;
      }
      
      const translateX = -currentSlide * slideWidth;
      carousel.style.transform = `translateX(${translateX}px)`;
      
      // Update button states
      updateCarouselButtons('plan', currentSlide, slideCount, slidesPerView);
    }

    // Function to slide prepaid plans
    function slidePrepaidPlans(direction) {
      const carousel = document.getElementById('prepaidCarousel');
      if (!carousel) return;
      
      const cards = carousel.children;
      const slideCount = cards.length;
      const slidesPerView = window.innerWidth < 768 ? 1 : 3;
      
      // Get current slide position
      const currentTransform = carousel.style.transform;
      let currentSlide = 0;
      if (currentTransform) {
        const match = currentTransform.match(/translateX\((-?\d+)px\)/);
        if (match && match[1]) {
          const cardWidth = cards[0]?.offsetWidth || 0;
          const gap = 16; // 1rem = 16px
          const slideWidth = cardWidth + gap;
          currentSlide = Math.round(Math.abs(parseInt(match[1])) / slideWidth);
        }
      }
      
      // Calculate the width of a single card including gap
      const cardWidth = cards[0]?.offsetWidth || 0;
      const gap = 16; // 1rem = 16px
      const slideWidth = cardWidth + gap;
      
      if (direction === 'next' && currentSlide < slideCount - slidesPerView) {
        currentSlide++;
      } else if (direction === 'prev' && currentSlide > 0) {
        currentSlide--;
      }
      
      const translateX = -currentSlide * slideWidth;
      carousel.style.transform = `translateX(${translateX}px)`;
      
      // Update button states
      updateCarouselButtons('prepaid', currentSlide, slideCount, slidesPerView);
    }

    // Function to update carousel button states
    function updateCarouselButtons(type, currentSlide, slideCount, slidesPerView) {
      const prevBtn = document.getElementById(`${type}CarouselPrev`);
      const nextBtn = document.getElementById(`${type}CarouselNext`);
      
      if (prevBtn) {
        prevBtn.disabled = currentSlide === 0;
        prevBtn.style.opacity = currentSlide === 0 ? '0.5' : '1';
      }
      
      if (nextBtn) {
        const maxSlide = slideCount - slidesPerView;
        nextBtn.disabled = currentSlide >= maxSlide;
        nextBtn.style.opacity = currentSlide >= maxSlide ? '0.5' : '1';
      }
    }

    // Call initializeCarousel when the page loads
    window.addEventListener('load', initializeCarousel);

    // Function to handle selecting a plan
    async function selectPlan(carrier, price, dataAmount, id, name) {
      try {
        console.log(`Selecting plan: ${carrier} - $${price} - ${dataAmount} - ${name}`);
        
        // Prepare the payload
          const payload = {
            carrier: carrier,
          price: parseFloat(price),
          data: dataAmount,
          id: id || '',
          plan_name: name || ''
        };
        
        // Make POST request to select_plan endpoint
        const baseUrl = window.location.origin;
        const response = await fetch(`${baseUrl}/select_plan`, {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify(payload)
        });
        
          if (!response.ok) {
          throw new Error('Failed to select plan');
        }
        
        const result = await response.json();
        
        if (result.success) {
          // Check if this is a redirect response (for Fido plans)
          if (result.redirect && result.url) {
            // For Fido plans, redirect to their website
            window.location.href = result.url;
          } else {
            // For all other carriers, continue with normal checkout flow
            window.location.href = `${baseUrl}/checkout`;
          }
        } else {
          console.error('Error selecting plan:', result);
          alert('There was an error selecting your plan. Please try again.');
        }
      } catch (error) {
        console.error('Error in selectPlan:', error);
        alert('There was an error processing your request. Please try again.');
      }
    }

    document.addEventListener('DOMContentLoaded', function() {
        // Smooth scroll for navigation links
        document.querySelectorAll('.scroll-to, #scroll-to-explore').forEach(link => {
            link.addEventListener('click', function(e) {
                e.preventDefault();
                let targetId;
                
                // If this is the scroll-to-explore button, target featured plans
                if (this.id === 'scroll-to-explore') {
                    targetId = '#featured-plans';
                } else {
                    targetId = this.getAttribute('href');
                }
                
                const targetElement = document.querySelector(targetId);
                if (targetElement) {
                    const headerOffset = 80; // Height of your fixed header
                    const elementPosition = targetElement.getBoundingClientRect().top;
                    const offsetPosition = elementPosition + window.pageYOffset - headerOffset;

                    window.scrollTo({
                        top: offsetPosition,
                        behavior: 'smooth'
                    });
                }
            });
        });

        // Enhanced scroll indicator interaction
        const scrollExplore = document.getElementById('scroll-to-explore');
        if (scrollExplore) {
            scrollExplore.addEventListener('mouseenter', function() {
                this.style.transform = 'translateY(-5px)';
                this.classList.add('opacity-100');
                this.classList.remove('opacity-60');
            });
            
            scrollExplore.addEventListener('mouseleave', function() {
                this.style.transform = 'translateY(0)';
                this.classList.remove('opacity-100');
                this.classList.add('opacity-60');
            });
        }

        // Hide scroll indicator when scrolling past hero section
        window.addEventListener('scroll', function() {
            const scrollExplore = document.getElementById('scroll-to-explore');
            const heroSection = document.getElementById('hero');
            
            if (scrollExplore && heroSection) {
                const heroBottom = heroSection.offsetTop + heroSection.offsetHeight;
                if (window.pageYOffset > heroBottom - window.innerHeight) {
                    scrollExplore.style.opacity = '0';
                } else {
                    scrollExplore.style.opacity = '0.6';
                }
            }
        });
    });

    // Add this to your existing JavaScript code in planB.html

    // Improve carousel functionality and prevent overflow
    document.addEventListener('DOMContentLoaded', function() {
      // Fix carousel overflow issues
      fixPlanCarouselOverflow();
      
      // Listen for window resize and orientation change events
      window.addEventListener('resize', function() {
        fixPlanCarouselOverflow();
      });
      
      window.addEventListener('orientationchange', function() {
        fixPlanCarouselOverflow();
      });
    });

    // Fix carousel overflow issues
    function fixPlanCarouselOverflow() {
      // Fix for plan carousel overflow
      const carouselContainers = document.querySelectorAll('.plan-carousel-container');
      if (carouselContainers.length > 0) {
        carouselContainers.forEach(container => {
          // Force overflow hidden
          container.style.overflow = 'hidden';
          container.style.width = '100%';
          container.style.position = 'relative';
          
          // Get the carousel
          const carousel = container.querySelector('.plan-carousel');
          if (carousel) {
            // Ensure carousel has width control
            carousel.style.width = '100%';
            carousel.style.position = 'relative';
            carousel.style.touchAction = 'pan-y'; // Restrict touch to vertical only
            
            // Reset transform to prevent overflow
            if (window.innerWidth <= 768) {
              // On mobile, ensure single card view
              carousel.style.transform = `translateX(0)`;
              
              // Reset current slide for mobile
              window.currentSlide = 0;
              
              // Update card widths for mobile
              const cards = carousel.querySelectorAll('.plan-card');
              if (cards.length > 0) {
                cards.forEach(card => {
                  card.style.flex = '0 0 100%';
                  card.style.maxWidth = '100%';
                  card.style.boxSizing = 'border-box';
                });
              }
            }
          }
        });
      }
      
      // Update carousel navigation visibility based on screen size
      const carouselButtons = document.querySelectorAll('.carousel-button');
      if (carouselButtons.length > 0) {
        const isMobile = window.innerWidth <= 768;
        carouselButtons.forEach(button => {
          // Adjust button position for different screen sizes
          if (isMobile) {
            if (button.classList.contains('prev')) {
              button.style.left = '0.5rem';
            } else {
              button.style.right = '0.5rem';
            }
            button.style.width = '2rem';
            button.style.height = '2rem';
          } else {
            if (button.classList.contains('prev')) {
              button.style.left = '-0.5rem';
            } else {
              button.style.right = '-0.5rem';
            }
            button.style.width = '3rem';
            button.style.height = '3rem';
          }
        });
      }
    }

    // Override the slidePlans function with a fixed version
    function slidePlans(direction, carouselType) {
      const carousel = document.getElementById(`${carouselType}Carousel`);
      if (!carousel) return;
      
      // Get all plan cards in the carousel
      const cards = carousel.querySelectorAll('.plan-card');
      if (cards.length === 0) return;
      
      // Calculate slide size based on visible cards
      const isMobile = window.innerWidth <= 768;
      const isTablet = window.innerWidth > 768 && window.innerWidth <= 1024;
      const visibleSlides = isMobile ? 1 : (isTablet ? 2 : 3);
      
      // Calculate max slide index (prevent overflow)
      const maxSlide = Math.max(0, cards.length - visibleSlides);
      
      // Update current slide with boundary checks
      if (direction === 'next') {
        window.currentSlide = Math.min(maxSlide, (window.currentSlide || 0) + 1);
      } else {
        window.currentSlide = Math.max(0, (window.currentSlide || 0) - 1);
      }
      
      // Calculate slide width and translate value
      const translateX = -window.currentSlide * (100 / visibleSlides);
      
      // Apply translation with smooth transition
      carousel.style.transition = 'transform 0.3s ease-out';
      carousel.style.transform = `translateX(${translateX}%)`;
      
      // Update button states
      updateCarouselButtons(carouselType, window.currentSlide, maxSlide);
    }

    // Update carousel button states
    function updateCarouselButtons(carouselType, currentSlide, maxSlide) {
      const prevBtn = document.querySelector(`.carousel-button.prev[onclick*="${carouselType}"]`);
      const nextBtn = document.querySelector(`.carousel-button.next[onclick*="${carouselType}"]`);
      
      if (prevBtn) {
        prevBtn.disabled = currentSlide <= 0;
        prevBtn.style.opacity = currentSlide <= 0 ? '0.5' : '1';
        prevBtn.style.cursor = currentSlide <= 0 ? 'not-allowed' : 'pointer';
      }
      
      if (nextBtn) {
        nextBtn.disabled = currentSlide >= maxSlide;
        nextBtn.style.opacity = currentSlide >= maxSlide ? '0.5' : '1';
        nextBtn.style.cursor = currentSlide >= maxSlide ? 'not-allowed' : 'pointer';
      }
    }
</script>

<script>
// Add these functions after your existing JavaScript

function showPlanDetailsModal() {
  const modal = document.getElementById('planDetailsModal');
  const modalContent = modal.querySelector('div');
  
  modal.classList.remove('hidden');
  modal.classList.add('flex');
  
  // Animate in
  setTimeout(() => {
    modalContent.style.transform = 'translate(0, 0)';
    modalContent.style.opacity = '1';
  }, 10);
}

function closePlanDetailsModal() {
  const modal = document.getElementById('planDetailsModal');
  const modalContent = modal.querySelector('div');
  
  // Animate out
  modalContent.style.transform = 'translate(0, -20px)';
  modalContent.style.opacity = '0';
  
  setTimeout(() => {
    modal.classList.remove('flex');
    modal.classList.add('hidden');
  }, 300);
}

// Update the chat functionality
document.addEventListener('DOMContentLoaded', function() {
  const chatForm = document.getElementById('chat-form');
  const chatInput = document.getElementById('chat-input');
  const chatMessages = document.getElementById('chat-messages');
  const planDetailsForm = document.getElementById('planDetailsForm');
  
  if (chatForm && chatInput && chatMessages) {
    chatForm.addEventListener('submit', async function(e) {
      e.preventDefault();
      
      const userMessage = chatInput.value.trim();
      if (!userMessage) return;
      
      // Add user message to chat
      addMessageToChat(userMessage, 'user');
      
      // Clear input
      chatInput.value = '';
      
      try {
        // Show loading indicator
        const loadingMessage = addMessageToChat('Finding the perfect plan for you...', 'ai', true);
        
        // Check if it's a greeting
        if (isGreeting(userMessage)) {
          loadingMessage.remove();
          addMessageToChat("Hello!  I'm Blue, your personal plan advisor. To help you better, I'll need some information about your current plan.", 'ai');
          showPlanDetailsModal();
          return;
        }
        
        // Make API call to get recommendations
        const response = await fetch('/chat', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({ userMessage })
        });
        
        if (!response.ok) {
          throw new Error('Failed to get recommendations');
        }
        
        const data = await response.json();
        
        // Remove loading message
        loadingMessage.remove();
        
        handleChatResponse(data);
        
      } catch (error) {
        console.error('Error:', error);
        addMessageToChat("I'm sorry, I encountered an error while processing your request. Please try again.", 'ai');
      }
    });
  }
  
  // Handle plan details form submission
  if (planDetailsForm) {
    planDetailsForm.addEventListener('submit', async function(e) {
      e.preventDefault();
      
      const formData = new FormData(planDetailsForm);
      const planDetails = {
        plan_price: parseFloat(formData.get('planPrice')),
        plan_data: parseFloat(formData.get('dataUsage')),
        carrier: formData.get('provider')
      };
      
      try {
        // Show loading indicator
        const loadingMessage = addMessageToChat('Analyzing your plan details...', 'ai', true);
        
        // Close modal
        closePlanDetailsModal();
        
        // Make API call with plan details
        const response = await fetch('/chat', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({
            userMessage: `Current plan: $${planDetails.plan_price}/month, ${planDetails.plan_data}GB with ${planDetails.carrier}`,
            planDetails: planDetails
          })
        });
        
        if (!response.ok) {
          throw new Error('Failed to process plan details');
        }
        
        const data = await response.json();
        
        // Remove loading message
        loadingMessage.remove();
        
        handleChatResponse(data);
        
      } catch (error) {
        console.error('Error:', error);
        addMessageToChat("I'm sorry, I encountered an error while processing your plan details. Please try again.", 'ai');
      }
      
      // Reset form
      planDetailsForm.reset();
    });
  }
  
  // Helper function to check if message is a greeting
  function isGreeting(message) {
    const greetings = ["hi", "hello", "hey", "good morning", "good afternoon", "good evening"];
    return greetings.some(greeting => message.toLowerCase().includes(greeting));
  }
  
  // Helper function to handle chat responses
  function handleChatResponse(data) {
    if (data.error) {
      addMessageToChat("I'm sorry, I encountered an error. Please try again.", 'ai');
      return;
    }
    
    // Create message container
    const messageDiv = document.createElement('div');
    messageDiv.className = 'flex items-start gap-3 max-w-[80%]';
    
    const avatarDiv = document.createElement('div');
    avatarDiv.className = 'w-8 h-8 rounded-full bg-accent flex items-center justify-center text-white shrink-0';
    avatarDiv.innerHTML = '<i class="ri-robot-line text-sm"></i>';
    
    const contentDiv = document.createElement('div');
    contentDiv.className = 'bg-white rounded-2xl rounded-tl-none p-4 shadow-md';
    
    // Add AI message
    const messageText = document.createElement('p');
    messageText.className = 'text-black mb-4';
    messageText.textContent = data.message;
    contentDiv.appendChild(messageText);
    
    // Add hint if present
    if (data.hint) {
      const hintDiv = document.createElement('div');
      hintDiv.className = 'mt-4 p-3 bg-accent/5 rounded-lg text-sm text-gray-600';
      hintDiv.textContent = data.hint;
      contentDiv.appendChild(hintDiv);
    }
    
    // Add recommended plans if present
    if (data.recommendedPlans && data.recommendedPlans.length > 0) {
      const plansContainer = document.createElement('div');
      plansContainer.className = 'space-y-4 mt-4';
      
      data.recommendedPlans.forEach(plan => {
        const planCard = document.createElement('div');
        planCard.className = 'bg-gray-50 rounded-xl p-4 border border-accent/10';
        
        const planContent = `
          <div class="flex justify-between items-center mb-2">
            <h4 class="font-display font-bold text-black">${plan.carrier} - ${plan.plan_name}</h4>
            <span class="text-accent font-bold">$${plan.plan_price}/mo</span>
          </div>
          <div class="text-sm text-gray-600">
            <p>${plan.plan_data} of data</p>
            <p>${plan.plan_features || 'Unlimited talk & text'}</p>
          </div>
          <button onclick="selectPlan('${plan.carrier}', ${plan.plan_price}, '${plan.plan_data}', '${plan.id}', '${plan.plan_name}')"
                  class="mt-3 w-full py-2 px-4 bg-accent text-white rounded-full hover:bg-accent/90 transition-colors text-sm">
            Select Plan
          </button>
        `;
        
        planCard.innerHTML = planContent;
        plansContainer.appendChild(planCard);
      });
      
      contentDiv.appendChild(plansContainer);
    }
    
    messageDiv.appendChild(avatarDiv);
    messageDiv.appendChild(contentDiv);
    
    chatMessages.appendChild(messageDiv);
    chatMessages.scrollTop = chatMessages.scrollHeight;
  }
});
</script>
